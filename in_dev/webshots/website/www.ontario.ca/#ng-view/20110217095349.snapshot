	var _tag=new WebTrends();
	var prod_hostnames = ["www.ontario.ca"];//to be tailored to each application,comma-separated
	var pixel_path = "images";//set this to the locatoin of the DEV-pixel's location on test host
	var dev_url = 'dev.media.gov.on.ca'; //hostname of dev pixel location

	function setSDCLocation(prod, current, dev){
		var isprod = 0; //assume no match
		for(i=0;i<prod.length; i++){
			if(current==prod[i]){
				isprod = 1;//verify that the page is from prod hostname
				break; //kill the for loop
			}
		}

		if(isprod){//if the domain is in production
			_tag.dcsCollect(); //call as normal
        }else{
			//wasn't a match for production value. send pixel request to devbox
			_tag.domain=dev;//override hostname with dev pixel info
			_tag.dcsid=pixel_path; //override dcsid with local path of dev pixel
			_tag.dcsCollect();
		}
	}//end setSDCLocation

	if(_tag != null && _tag != "undefined") {
		setSDCLocation(prod_hostnames,window.location.hostname.toLowerCase(),dev_url);
	}
//]]>
</script>
